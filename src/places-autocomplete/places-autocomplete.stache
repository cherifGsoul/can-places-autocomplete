<can-import from="./places-autocomplete-callback/places-autocomplete-callback" />

<can-slot name="places-autocomplete-input">
  <input can-places-autocomplete-callback value:from="suggestion"/>
</can-slot>

{{#if(suggestions.length)}}
  <div class="places-autocomplete__suggestions">
    <can-slot name="places-autocomplete-suggestions">
      {{#each(suggestions, suggestion=value)}}  
        <div>{{suggestion.description}}</div>  
      {{/each}}
    </can-slot>
  </div>
{{/if}}